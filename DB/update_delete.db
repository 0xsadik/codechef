-- let's update and delete stuffs 

UPDATE workers
SET hourly_pay = 10.23,
	hire_date = "2023-04-01"
WHERE employee_id = 5;

SELECT * FROM workers;


-- set specific thing null 

UPDATE workers
SET hire_date = NULL,
	last_name = "Khatun"
WHERE employee_id = 5;

SELECT * FROM workers;



-- update specific columns rows 

UPDATE workers
SET hourly_pay = 10.25;

SELECT * FROM workers;


-- delete a row 

DELETE FROM workers
WHERE employee_id = 5;
SELECT * FROM workers;

