-- select data from a table 

SELECT first_name, last_name 
FROM workers;


-- another way 

SELECT last_name, first_name
FROM workers;

-- find specific id using a clause " WHERE " 

SELECT *
FROM workers
WHERE employee_id = 5;


-- find some data using name 

SELECT * 
FROM workers
WHERE first_name = "ahm";


-- conditional finding 

SELECT * 
FROM workers
WHERE hourly_pay >= 15;



